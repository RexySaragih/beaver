{
  "name": "beaver",
  "version": "1.0.0",
  "description": "Business Engineering And Visual Enterprise Representation - A local business flow diagram tool",
  "scripts": {
    "copy-assets": "cpx \"src/client/assets/ico/**/*\" public/assets/ico",
    "start": "webpack serve --mode development",
    "build": "npm run copy-assets && webpack --mode production",
    "server": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "mcp": "ts-node-dev --respawn --transpile-only src/server/mcpServer.ts",
    "dev": "npm run copy-assets && concurrently \"npm run start\" \"ts-node src/server/start.ts\"",
    "dev:debug": "npm run copy-assets && concurrently \"npm run start\" \"npm run server\" \"npm run mcp\"",
    "test:analysis": "ts-node src/server/test-local-analysis.ts",
    "models:list": "ts-node src/server/utils/modelChecker.ts list",
    "models:current": "ts-node src/server/utils/modelChecker.ts current",
    "models:test": "ts-node src/server/utils/modelChecker.ts test"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.17.3",
    "@modelcontextprotocol/sdk": "^1.15.1",
    "@types/axios": "^0.9.36",
    "@types/cors": "^2.8.19",
    "@types/socket.io": "^3.0.1",
    "@types/socket.io-client": "^1.4.36",
    "@types/uuid": "^10.0.0",
    "axios": "^1.10.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "docx": "^9.5.1",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "html2pdf.js": "^0.10.3",
    "ioredis": "^5.6.1",
    "mcp-server": "^0.0.9",
    "model-context-protocol": "^1.0.2",
    "multer": "^1.4.5-lts.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.1",
    "redis": "^5.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "styled-components": "^6.1.8",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.19.7",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "concurrently": "^8.2.2",
    "cpx": "^1.5.0",
    "css-loader": "^6.10.0",
    "html-webpack-plugin": "^5.6.0",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  }
}
